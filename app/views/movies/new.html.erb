<h1>Cageflix</h1>
<h2>Netflix for Cage Movies</h2>

<%= form_for @movie do |f| %>
 <% @movie.errors.full_messages.each do |error| %>
 <ul>
   <div class="error"><%= error %> </div>
 </ul>
    <% end %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :year %>
  <%= f.text_field :year %>
  <%= f.label :synopsis %>
  <%= f.text_field :synopsis %>
  <%= f.submit "Submit" %>
<% end %>
